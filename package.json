{
  "name": "planetcnc-expression",
  "displayName": "PlanetCNC Expression",
  "description": "Support for Planet CNC Expr.txt files",
  "version": "0.0.2",
  "publisher": "styletronix.net",
  "private": true,
  "license": "MIT",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
"activationEvents": [
  "onLanguage:planetcncexpr"
],
  "main": "./extension.js",
  "devDependencies": {
    "@types/node": "^16.18.34",
    "@types/vscode": "^1.73.0"
  },
  "contributes": {
    "languages": [{
      "id": "planetcncexpr",
      "aliases": ["PlanetCNC Expression", "planetcncexpr"],
      "extensions": [".txt"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "planetcncexpr",
      "scopeName": "source.expression",
      "path": "./syntaxes/planetcncexpr.tmLanguage.json"
    }]
  }
}
